{
  "name": "base-setup-with-login-boilerplate",
  "version": "1.0.0",
  "description": "Base setup architecture with sign in module ",
  "author": "Twilight IT Solutions",
  "license": "MIT",
  "main": "src/app.js",
  "engines": {
    "node": ">=18.16.0"
  },
  "scripts": {
    "test": "SET NODE_ENV=local&& jest",
    "seed:windows:local": "SET NODE_ENV=local&& npx sequelize-cli db:seed:all",
    "seed:linux:local": "export NODE_ENV=local&& npx sequelize-cli db:seed:all",
    "migrate:windows:local": "SET NODE_ENV=local&& npx sequelize-cli db:migrate",
    "migrate:linux:local": "export NODE_ENV=local&& npx sequelize-cli db:migrate",
    "start:windows:local": "SET NODE_ENV=local&& nodemon src/app.js",
    "start:linux:local": "export NODE_ENV=local&& nodemon src/app.js",
    "seed:windows:dev": "SET NODE_ENV=dev&& npx sequelize-cli db:seed:all",
    "seed:linux:dev": "export NODE_ENV=dev&& npx sequelize-cli db:seed:all",
    "migrate:windows:dev": "SET NODE_ENV=dev&& npx sequelize-cli db:migrate",
    "migrate:linux:dev": "export NODE_ENV=dev&& npx sequelize-cli db:migrate",
    "start:windows:dev": "SET NODE_ENV=dev&& pm2 start src/app.js",
    "start:linux:dev": "export NODE_ENV=dev&& pm2 start src/app.js",
    "seed:windows:staging": "SET NODE_ENV=staging&& npx sequelize-cli db:seed:all",
    "seed:linux:staging": "export NODE_ENV=staging&& npx sequelize-cli db:seed:all",
    "migrate:windows:staging": "SET NODE_ENV=staging&& npx sequelize-cli db:migrate",
    "migrate:linux:staging": "export NODE_ENV=staging&& npx sequelize-cli db:migrate",
    "start:windows:staging": "SET NODE_ENV=staging&& pm2 start src/app.js",
    "start:linux:staging": "export NODE_ENV=staging&& pm2 start src/app.js",
    "seed:windows:prod": "SET NODE_ENV=prod&& npx sequelize-cli db:seed:all",
    "seed:linux:prod": "export NODE_ENV=prod&& npx sequelize-cli db:seed:all",
    "migrate:windows:prod": "SET NODE_ENV=prod&& npx sequelize-cli db:migrate",
    "migrate:linux:prod": "export NODE_ENV=prod&& npx sequelize-cli db:migrate",
    "start:windows:prod": "SET NODE_ENV=prod&& pm2 start src/app.js",
    "start:linux:prod": "export NODE_ENV=prod&& pm2 start src/app.js",
    "start:pm2": "pm2-runtime start src/app.js",
    "start:nodemon": "nodemon src/app.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "prepare": "husky install"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "!coverage/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lillastar824/Avalbl_Backend.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "nodejs",
    "rest",
    "api",
    "sequelize",
    "mysql"
  ],
  "devDependencies": {
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
