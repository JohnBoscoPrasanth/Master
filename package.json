    "sequelize": "^6.31.0",
    "sib-api-v3-sdk": "^8.5.0",
    "superagent": "^8.0.9",
    "supertest": "^6.3.3",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "bugs": {
